{
  "name": "YES Registration Helper",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This Extension is a Registration Helper that can load and allow configuration of the Schedule for Vanderbilt Students on other webpages",
  "homepage_url": "https://github.com/paytonod/yesregistrationhelper",
  "icons": {
    "16": "icons/favicon.png",
    "48": "icons/favicon.png",
    "128": "icons/favicon.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/favicon.png",
    "default_title": "YES Registration Helper"
  },
  "permissions": [
    "contentSettings",
    "contextMenus",
    "cookies",
    "history",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [{
    "matches": [
      "https://www.google.com/*"
    ],
    "js": [
      "src/inject/inject.js",
      "src/inject/schedule.js"
    ]
  }]
}
