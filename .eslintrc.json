{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "no-console": 0,
    "no-alert": 0,
    "no-plusplus": 0,
    "no-restricted-globals": 0,
    "no-unexpected-multiline": 0,
    "no-undef": 0,
    "linebreak-style": 0,
    "no-use-before-define": 0,
    "no-unused-expressions": 0
  }
}
